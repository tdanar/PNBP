{
    "name": "emotality/tawk-laravel",
    "description": "A tawk.to chat widget for Laravel.",
    "keywords": ["tawk.to", "tawkto", "tawk", "chat", "widget", "laravel"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jean-Pierre Fourie",
            "email": "jp@emotality.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.0"
    },
    "autoload": {
        "psr-4": {
            "Emotality\\TawkTo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Emotality\\TawkTo\\TawkToServiceProvider"
            ],
            "aliases": {
                "TawkTo": "Emotality\\TawkTo\\Facades\\TawkTo"
            }
        }
    }
}
